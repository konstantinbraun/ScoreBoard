@inherits IdentitySample.Pages.MyBasePage
@{
    ViewBag.Title = IdentitySample.Resx.AppResource.Tournament;
}

@Html.Partial("_PageHeader", Model)
@Html.Partial("_PageTabs", this.NavigationService.GetTabs(ViewContext.RequestContext.RouteData, ViewContext.RequestContext.HttpContext.Request["active"]))

@section Scripts {
    @Scripts.Render("~/bundles/countdown")
    @Scripts.Render("~/bundles/sms")

    @{
        var key = IdentitySample.Properties.Settings.Default.googleMapKey;
        <script src='@string.Format("https://maps.googleapis.com/maps/api/js?key ={0}&sensor =TRUE_OR_FALSE&callback =initialize", key)'></script>
    }
}